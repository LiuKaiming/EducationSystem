<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.qdu.mapping.TeacherFeedBackMapping">
	<resultMap type="com.qdu.pojo.TeacherFeedBack" id="TeacherFeedBackResult">
		<id property="fid" column="FID" />
		<result property="foption" column="FOption" />
		
		
		<association property="teacherFeedBackQuestion" javaType="com.qdu.pojo.TeacherFeedBackQuestion">
			<id property="qid" column="QID" />
			<result property="qcontent" column="QContent" />
			
			<association property="teacher" javaType="com.qdu.pojo.Teacher">
			<id property="teacherId" column="TeacherID" />
			<result property="teacherName" column="TeacherName" />
			<result property="teacherPwd" column="TeacherPwd" />
			<result property="teacherGender" column="TeacherGender" />
			</association>
			
		</association>
		
				<association property="student" javaType="com.qdu.pojo.Student">
			<id property="studentId" column="StudentID" />
			<result property="studentName" column="StudentName" />
			<result property="studentPwd" column="StudentPwd" />
			<result property="studentGender" column="StudentGender" />
			<result property="studentGrade" column="StudentGrade" />
			
			<association property="batch" javaType="com.qdu.pojo.Batch">
			<id property="cid" column="CID" />
			<result property="cyear" column="CYear" />
			<result property="cname" column="CName" />
			</association>
			
		</association>
		
		
	</resultMap>

</mapper>

